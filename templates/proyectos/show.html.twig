{% extends 'base.html.twig' %}

{% block title %}Proyectos{% endblock %}

{% block body %}
    <h1>Proyectos</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ proyecto.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ proyecto.name }}</td>
            </tr>
            <tr>
                <th>Explication</th>
                <td>{{ proyecto.explication }}</td>
            </tr>
            <tr>
                <th>Demo</th>
                <td>{{ proyecto.demo }}</td>
            </tr>
            <tr>
                <th>Github</th>
